<a class="cell cell--service" data-bp="1100" href="{{ page.url }}">

	<h2 class="headline--service color-{{ page.header.page_color }}"><span class="icon--service  icon--{{page.title }}"></span><span class="font-light">toctoc</span><span>{{ page.title|replace({"toctoc": ""}) }}</span></h2>

	<h4 class="byline--service color-{{ page.header.page_color }}">{{ page.header.subtitle_short }}</h4>
	<p>{{ page.header.blurb_short }}</p>

	<span class="btn btn-{{ page.header.page_color|trim('0..9') }} btn-m"><i></i><strong>{% if langswitcher.current == 'fr' %}En savoir +{% elseif langswitcher.current == 'en' %}Tell me more{% endif %}</strong></span>


</a>