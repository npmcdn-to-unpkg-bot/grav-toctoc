<div class="press-article">
	<h3><a href="{{ page.header.link }}" target="_new">{{ page.header.headline }}</a><span class="press-article__date">{{ page.header.date }}</span></h3>
	
	<div class="cell cell-in-row cell-67">
		{{ page.content}}
	</div>

	<div class="cell cell-in-row cell-33">
		<a href="{{ page.header.link }}" target="_new">{{ page.media[page.header.image].cropResize(300, 300).html() }}</a>
	</div>
	
</div>