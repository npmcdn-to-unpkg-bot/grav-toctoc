<section class="block block--stretch  block--partner">
	
	<div class="block">
	{{ page.content }}


	<ul id="filters" class="filter" data-option-key="filter">
    <li><a href="#filter" data-option-value="*" class="selected">Show all</a></li>
    {% for item in page.header.categories %}
        <li><a href="#filter" data-option-value=".{{ item.name }}">{{ item.name }}</a></li>
    {% endfor %}
	</ul>


		{% for partner in page.header.partners %}
			<a class="card  card--partner  width-25" href="{{ partner.link }}" target="_new">

				<div class="card__img" href="{{ partner.link }}" target="_new">
					{{ page.media[partner.image].cropResize(300, 300).html() }}
				</div>

				<div class="card__info">
					<h3>{{ partner.name }}</h3>
					<p>{{ partner.text }}</p>
				</div>

			</a>
		{% endfor %}

	</div>

</section>